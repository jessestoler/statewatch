{
  "name": "voterbooth",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"nodemon server.js\" \"cd client && yarn start\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && node server.js && npm install --only=dev --no-shrinkwrap && npm run build"
    
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "mongoose": "^5.2.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-google-charts": "^1.6.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
